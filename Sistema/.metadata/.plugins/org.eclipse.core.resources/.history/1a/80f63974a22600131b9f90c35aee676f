package snippet;

public class Snippet {
	create table Alunos
	(
	NOME varchar(60),
	IDADE varchar(2),
	Endereco varchar(60));
	
	create table Treinos
	(
	NOME varchar(60),
	Series varchar(1),
	Repeticao varchar(2));
	
	
	
	insert into Alunos
	VALUES ('Roberto','26','Rua Feliciano 1269');
	insert into Alunos
	VALUES ('Paula','20','Avenida Gregoriano 853 AP 02');
	insert into Alunos
	VALUES ('Jorge','36','Rua Carlos Almeida 963');
	insert into Alunos
	VALUES ('Julia','19','Avenida Sarmen 153');
	
	insert into Treinos
	VALUES ('Remada Aberta','3','12');
	insert into Treinos
	VALUES ('Supino Reto','3','12');
	insert into Treinos
	VALUES ('Triceps Corda','3','12');
	insert into Treinos
	VALUES ('Elevacao Lateral','3','12');
	
	select * from Alunos;
}

